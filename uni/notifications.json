{
	"out": {
		"ios": "../ios/ios-notification/SbisNotifications/SbisNotifications/Code",
		"//android": "out/android/notifications"
	},
	"storage": {
		".name": "Notifications",
		".version": 3,
		".folders": { 
			"iconCachePath": "IconCache"
		},
		"Notification": {
			"id": "uuid!",
			"type": "int",
			"date": "dateTime",
			"link": "string",
			"targetId": "uuid",
			"readLink": "string",
			"iconLink": "string",
			"header1": "string",
			"header2": "string",
			"text": "string",
			"details": "string",
			"commentText": "string",
			"commentStatus": "int",
			"resultVersion": "string",
			"resultTerm": "dateTime",
			"resultImportant": "int",
			"resultText": "string",
			"resultStatus": "int",
			"read": "bool",
			"updates": "int"
		},
		"NotificationTypeSummary": {
			"type": "int!",
			"count": "int"
		},
		"NotificationsSummary": {
			"lastSyncTime": "dateTime"
		},
		".queries": {
			"getTypeSummaries": {
				".select": "NotificationTypeSummary",
				".returns": "array"
			},
			"getTypeSummary": {
				"type": "int",
				".select": "NotificationTypeSummary",
				".where": "type = ?",
				".returns": "record"
			},
			"getNotificationTypeById": {
				"id": "uuid",
				".select": "Notification(type)",
				".where": "id = ?",
				".returns": "record"
			},
			"iterateNotifications": "Notification",
			"iterateNotificationIdAndType": "Notification(id,type)",
			"iterateNotificationByType": {
				"type": "int",
				".select": "Notification",
				".where": "type = ?",
				".orderby": "date desc"
			}
		}
	},
	"cloud": {
		".protocol": 4,
		"Notifications": {
			".url": "notice/service",
			"Notification.ListApi ~ getNotificationList": {
				"params": {
					"Filter": {
						".modifier": "?",
						"ProfileUUID ~ profileId": "uuid",
						"Type": "int",
						"Limit": "int",
						"LimitToType": "int",
						"Page": "int",
						"PageSize": "int"
					}
				},
				"result": {
					".type": "GetNotificationListResult",
					".modifier": "*",
					"GUID ~ id": "uuid",
					"Type": "int",
					"Count": "int",
					"Date": "dateTime",
					"UserId": "int",
					"Params": {
						".encoding": "jsonstring",
						"Link": "string",
						"ReadLink": "string",
						"IconLink": "string",
						"UUID ~ targetId": "uuid", 
						"Render": {
							"Header1": {
								"Text": "string",
								"Type": "int"
							},
							"Header2": "string",
							"Text": "string",
							"Details": "string",
							"Comment": {
								"Text": "string",
								"Status": "int"
							},
							"Result": {
								"Version": "string",
								"Term": "dateTime",
								"Important": "int",
								"Text": "string",
								"Status": "int",
							}
						}
					}
				}
			}
		}
	}
}

